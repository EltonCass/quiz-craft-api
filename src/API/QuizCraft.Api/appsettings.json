{
  "AllowedHosts": "*",
  "ExternalServices": {
    "OpenAPIKey": "YOUR_API_KEY"
  },
  "SecurityToken": "SECURITY_TOKEN",
  "ConnectionStrings": {
    "ApplicationInsights": "APPLICATION_INSIGHTS_CONNECTION_STRING",
    "QuizAPIConnectionString": "CONNECTION_STRING"
  },
  "EmailSettings": {
    "ApiKey": "",
    "FromName": "",
    "FromAddress": "User@Domain.tech"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
